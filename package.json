{
  "name": "fitbit-oauth2-client",
  "version": "0.0.12",
  "description": "Fitbit client API for OAuth2",
  "main": "index.js",
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/test/*.js",
      "!<rootDir>/test/**/*.js",
      "!<rootDir>/node_modules/**",
      "!<rootDir>/coverage/**",
      "!<rootDir>/index.js"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "./node_modules/.bin/jest --coverage",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "keywords": [
    "fitbit",
    "oauth2"
  ],
  "author": "Björn Sjögren",
  "contributors": [
    "Andrew Peebles"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lokanx/fitbit-oauth2-client.git"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "qs": "^6.9.4",
    "simple-oauth2": "^4.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "axios-mock-adapter": "^1.19.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.3",
    "jest": "^26.6.3"
  }
}
